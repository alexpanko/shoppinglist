{{!-- Hero2 small screen size--}}
<div class="container">
  <div class="row">
    <div class="col">
      <section class="jumbotron jumbotron-fluid pb-1 myHeaderArticle" style="background-image: linear-gradient(to top, rgb(253, 203, 110, 0.8), rgba(253, 203, 110, 0)50%), url({{values.0.listImage}});" >
        <div class="container">
          <div class="row">
            <h1 class="display-2 text-light font-weight-bold myHeaderTitle myBgColor-GreenDarnerTail text-white mb-5 mx-3">{{values.0.listName}}</h1>
          </div>
        </div>
      </section>
    </div>
  </div>
</div>

{{!-- Section One --}}
<section class="container py-3" id="sectionOne">
  <div class="row">
    <div class="col-md-8 mt-2">
      <h1>{{values.0.listName}}</h1>
      <h4 class="text-secondary mb-2">{{values.0.listDescription}}</h4>
      {{#if user}}
        <div class="text-center pt-3"><a class="btn btn-primary px-5" href="/items/copylist/{{values.0.id}}" role="button">Copy this list to make changes</a></div>
      {{/if}}
      {{#each values.[1]}}
      <h4 class="font-weight-bold mb-3 mt-5">{{itemName}}</h4>
      <img class="w-100" src="{{itemImage}}">
      <p class="pt-4">{{itemNotes}}</p>
      <p class="text-truncate" style="max-width: 500px"><strong>URL: </strong><a href="{{itemUrl}}">{{itemUrl}}</a></p>
      {{/each}}
      {{#if user}}
        <div class="text-center pt-3"><a class="btn btn-primary px-5" href="/items/copylist/{{values.0.id}}" role="button">Copy this list to make changes</a></div>
      {{/if}}
    </div>
    <div class="col-md-4 mt-2 pl-5 myHideOnMob">
      <div class="myExploreMiniCards">
        <h3>New articles:</h3>
        {{> exploremoremini}}
      </div>
    </div>
  </div>
</section>

{{!-- Section Explore More--}}
<section class="container mt-5">
  <hr>
  <div class="row">
    <div class="col text-center pb-4">
      <h1 class="myColor-BrightYarrow">Explore more...</h1>
    </div>
  </div>
  {{> exploremore}}
</section>